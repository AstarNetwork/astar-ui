{
  "name": "@astar-network/astar-ui",
  "version": "0.0.104",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/AstarNetwork/astar-ui.git"
  },
  "files": [
    "dist/*"
  ],
  "main": "dist/astar-ui.umd.js",
  "module": "dist/astar-ui.es.js",
  "types": "dist/types/index.d.ts",
  "typings": "dist/types/index.d.ts",
  "scripts": {
    "clean": "rm -rf node_modules",
    "build:types": "rm -rf ./node_modules/@types/react && vue-tsc --project tsconfig.build-types.json --declaration --emitDeclarationOnly --outDir dist/types ",
    "build:lib": "vue-tsc --noEmit && vite build --config ./build/lib.config.ts && npm run build:types",
    "storybook": "NODE_OPTIONS=--openssl-legacy-provider && storybook dev -p 6006",
    "build-storybook": "storybook build",
    "chromatic": "npx chromatic --project-token=af2bfd147de5 --auto-accept-changes",
    "dev": "vite --config ./build/base.config.ts"
  },
  "dependencies": {
    "@iconify-prerendered/vue-ic": "^0.23.1672653513",
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@babel/core": "^7.22.10",
    "@storybook/addon-actions": "^7.3.2",
    "@storybook/addon-essentials": "^7.3.2",
    "@storybook/addon-links": "^7.3.2",
    "@storybook/addon-mdx-gfm": "^7.3.2",
    "@storybook/addons": "^7.3.2",
    "@storybook/vue3": "^7.3.2",
    "@storybook/vue3-vite": "^7.3.2",
    "@types/node": "^20.5.4",
    "@vitejs/plugin-vue": "^4.3.3",
    "babel-loader": "^9.1.3",
    "chromatic": "^6.23.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.66.1",
    "storybook": "^7.3.2",
    "storybook-dark-mode": "^3.0.1",
    "typescript": "^5.1.6",
    "vite": "^4.4.9",
    "vite-plugin-copy": "^0.1.6",
    "vite-plugin-dts": "^3.5.2",
    "vite-plugin-md": "^0.21.5",
    "vue-loader": "^17.2.2",
    "vue-router": "^4.2.4",
    "vue-tsc": "^1.8.8"
  },
  "packageManager": "yarn@3.6.3"
}
